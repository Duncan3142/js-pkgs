{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["package.json", ".env"],
	"pipeline": {
		"//#clean": {
			"outputs": [],
			"dependsOn": [],
			"cache": false
		},
		"clean": {
			"outputs": [],
			"dependsOn": [],
			"cache": false
		},
		"//#format": {
			"outputs": [],
			"dependsOn": []
		},
		"format": {
			"outputs": [],
			"dependsOn": []
		},
		"format:fix": {
			"outputs": [],
			"dependsOn": []
		},
		"assets": {
			"outputs": [],
			"dependsOn": ["^assets"]
		},
		"test": {
			"outputs": [],
			"dependsOn": ["assets"]
		},
		"build": {
			"outputs": [],
			"dependsOn": ["//#format", "format", "assets", "test"]
		}
	}
}
