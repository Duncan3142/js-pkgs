{
	"name": "@duncan3142/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Duncan3142/mono.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "restricted"
	},
	"description": "ESLint config",
	"keywords": [],
	"version": "0.0.0",
	"author": "Duncan Giles",
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": ">=22",
		"npm": ">=11"
	},
	"main": "./eslint.config.js",
	"exports": {
		".": {
			"import": "./eslint.config.js",
			"types": "./eslint.config.d.ts"
		}
	},
	"files": [
		"eslint.config.js",
		"eslint.config.d.ts"
	],
	"scripts": {
		"clean": "rm -rf .prettier .package .tsc .eslint",
		"format": "prettier --check --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"format:fix": "prettier --write --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"assets": "tsc --build",
		"assets:watch": "tsc --build --watch",
		"test:tsc": "tsc --build --noEmit",
		"test:eslint": "eslint --max-warnings 0 --config .tsc/dist/eslint.config.js .",
		"test:eslint:fix": "eslint --fix --config .tsc/dist/eslint.config.js .",
		"inspect:eslint": "eslint --inspect-config --config .tsc/dist/eslint.config.js ."
	},
	"peerDependencies": {
		"@types/eslint": "^9.6.1",
		"eslint": "^9.17.0"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
		"@eslint/compat": "1.2.4",
		"@eslint/js": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsdoc": "50.6.1",
		"eslint-plugin-no-secrets": "^2.1.1",
		"eslint-plugin-promise": "^7.2.1",
		"typescript-eslint": "^8.19.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.11",
		"@duncan3142/prettier-config": "^0.1.1",
		"@duncan3142/tsc-config": "^0.1.1",
		"@eslint/config-inspector": "^0.7.0",
		"@types/eslint": "^9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"eslint": "^9.17.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2"
	},
	"volta": {
		"node": "22.12.0",
		"npm": "11.0.0"
	}
}
